<h1>參賽者列表</h1>
<table class="table table-hover">
  <thead>
    <tr>
      <th>id</th>
      <th>名字</th>
      <th></th>
    </tr>
  </thead>

  <% @users.each do |user| %>
      <tr>
        <td>
          <%= user.id %>
        </td>
        <td>
          <%= user.name %>
        </td>
        <td>
          <%= link_to "參賽者資訊", user_path(user) %>
        </td>
      </tr>
  <% end %>
</table>
